{
  "name": "ratemylandlord",
  "version": "1.0.0",
  "description": "Application that allows users to rate and review previous and current landlords",
  "main": "server/app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server/app.js",
    "server": "nodemon server/app.js",
    "client": "npm run serve --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Brendan DeLancey",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "aws-sdk": "^2.1028.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.13.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
